{
  "$schema": "http://json-schema.org/schema#", 
  "type": "object", 
  "properties": {
    "username": {
      "type": "string"
    }, 
    "network_mappings": {
      "type": "string"
    }, 
    "broker_type": {
      "enum": [
        "qpid", 
        "rabbit_mq"
      ], 
      "type": "string"
    }, 
    "address": {
      "type": "string"
    }, 
    "password": {
      "type": "string"
    }, 
    "port": {
      "type": "integer", 
      "format": "int32"
    }
  }
}